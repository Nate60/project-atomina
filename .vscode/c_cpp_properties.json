{
  "configurations": [
    {
      "name": "linux-gcc-x64",
      "includePath": [
        "${workspaceFolder}/**",
        "${workspaceFolder}/vendor/SFML/include"
      ],
      "compilerPath": "/usr/bin/gcc",
      "cStandard": "${default}",
      "cppStandard": "${default}",
      "intelliSenseMode": "linux-gcc-x64",
      "compilerArgs": [
        "-lsmfl-graphics",
        "-lsfml-window",
        "-lsfml-network",
        "-lsfml-system",
        "-lsfml-audio",
        "-L${workspaceFolder}/vendor/SFML/lib"
      ]
    }
  ],
  "version": 4
}